.navs
	width:200px
	position:fixed
	left:0
	top:0
	bottom:0
	background:#324157
	overflow:auto
	&::-webkit-scrollbar
		width:0
	.el-menu .fa
		margin-right:5px
	.logo
		height:60px
		padding:10px
		i
			display:block
			margin:auto
			width:60px
			height:60px
			border-radius:60px
			background:#fff
.main
	margin-left:200px
	padding:10px 10px 50px 10px
	.el-form-item
		margin-bottom:0
	.main-title
		border-bottom:2px solid #bfcbd9
		height:40px
		line-height:40px
		color:#324157
		font-size:16px !important
		margin-bottom:10px
		.el-input
			width:200px
			float:right
	.main-cover
		form
			display:inline-block
			width:100px
			height:30px
			line-height:30px
			border:1px solid #bfcbd9
			border-radius:4px
			text-align:center
			vertical-align:top
			position:relative
			input
				cursor:pointer
				opacity:0
				position:absolute
				top:0
				left:0
				z-index:2
				width:100px
				height:40px
		.cover
			width:120px
			height:80px
			display:inline-block
			vertical-align:top
			border:1px solid #bfcbd9
			img
				max-width:120px
				max-height:80px
	.main-pagation
		position:fixed
		bottom:0
		right:0
		left:200px
		height:26px
		z-index:2
		padding:7px 0
		background:#fff
		border-top:1px solid #bfcbd9
		.el-pagination
			float:right